{
  "name": "pagopa-interop-probing-eservice-operations",
  "version": "1.0.0",
  "description": "PagoPA probing eservice operations",
  "main": "dist/src",
  "type": "module",
  "scripts": {
    "test": "vitest --config ./test/vitest.config.ts",
    "test:it": "vitest db --config ./test/vitest.config.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "node --watch --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
    "build": "tsc",
    "generate-model": "mkdir -p ./src/model/generated && pnpm openapi-zod-client './open-api/api-eservice-operations-interop-v1.yaml' -o './src/model/generated/api.ts'",
    "clean-generated": "pnpm exec rm ./src/model/generated/api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@testcontainers/postgresql": "^10.2.2",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "testcontainers": "^10.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3"
  },
  "dependencies": {
    "@zodios/core": "^10.9.2",
    "@zodios/express": "^10.6.1",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "openapi-zod-client": "^1.7.1",
    "pagopa-interop-probing-commons": "workspace:*",
    "pagopa-interop-probing-models": "workspace:*",
    "pagopa-interop-probing-eservice-operations-client": "workspace:*",
    "pg": "^8.11.3",
    "ts-pattern": "^5.0.1",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  }
}