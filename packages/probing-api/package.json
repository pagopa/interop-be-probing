{
  "name": "pagopa-interop-probing-api",
  "version": "1.0.0",
  "description": "PagoPA Interoperability api gateway probing",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:it": "vitest db",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "node --watch --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
    "build": "tsc",
    "generate-model": "mkdir -p ./src/model/generated",
    "clean-generated": "pnpm exec rm ./src/models/generated/api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@pagopa/eslint-config": "^3.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "pagopa-interop-probing-commons": "workspace:*",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "ts-pattern": "^5.0.1",
    "zod": "^3.21.4"
  }
}
