    "@types/node": "^20.3.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.536.0",
    "@tsconfig/node-lts": "^20.1.0",
    "dotenv-flow": "^3.2.0",
    "pagopa-interop-probing-commons": "workspace:*",
    "pagopa-interop-probing-models": "workspace:*",
    "ts-pattern": "^5.0.1",
    "zod": "^3.21.4"
